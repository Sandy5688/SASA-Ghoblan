PORT=5002
NODE_ENV=production
SERVICE_NAME=soundcloud-service

# Demo mode
SOUNDCLOUD_DEMO_MODE=true

# SoundCloud credentials (10 accounts)
SOUNDCLOUD_CLIENT_ID_1=your_client_id_here
SOUNDCLOUD_CLIENT_SECRET_1=your_client_secret_here
SOUNDCLOUD_ACCESS_TOKEN_1=your_access_token_here
# ... up to 10 accounts
SOUNDCLOUD_CLIENT_ID_10=
SOUNDCLOUD_CLIENT_SECRET_10=
SOUNDCLOUD_ACCESS_TOKEN_10=

UPLOADS_DIR=/app/uploads
LOGS_DIR=/app/logs
DASHBOARD_RELAY_URL=http://dashboard_relay:5010/api/logs